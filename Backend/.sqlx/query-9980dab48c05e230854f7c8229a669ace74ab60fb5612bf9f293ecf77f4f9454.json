  "query": "\n            SELECT fee_type, fee_value\n            FROM transaction_fees \n            WHERE transaction_type = $1 AND is_active = true\n            AND min_amount <= $2 AND max_amount >= $2\n            ORDER BY min_amount DESC\n            LIMIT 1\n            ",
